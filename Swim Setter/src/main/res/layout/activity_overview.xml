<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    tools:context="com.tygrdevelopment.swimsetter.swimsetter.swimsetter.Overview">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/my_sets"
        android:text="@string/my_sets"
        android:paddingLeft="15dp"
        android:paddingStart="15dp"
        android:paddingRight="0dp"
        android:paddingEnd="0dp"
        android:textSize="30sp"/>
    <View
        android:layout_width="150dp"
        android:layout_height="10dp"
        android:background="#000000"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight=".9">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/set_container"
            android:background="#000000"
            android:divider="?android:attr/dividerHorizontal"
            android:dividerPadding="20dp"
            android:showDividers="middle">

        </TableLayout>

    </ScrollView>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#000000"/>

    <Button
        android:layout_width="match_parent"
        android:background="@color/button"
        android:id="@+id/new_set_button"
        android:layout_height="0dp"
        android:layout_weight=".1"
        android:text="@string/create_new_set"/>


</LinearLayout>
